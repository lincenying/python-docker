{% extends 'base.twig' %}

{% block intro %}
    {% if message %}
        {{ message }}<br>
        <img src="{{ url }}" alt="" width=="200" height="200">
    {% else %}
        <p>请选择上传文件</p>
    {% endif %}

    <table>
        {% for val in users %}
            <tr>
                <td>{{ val._id }}</td>
                <td>{{ val.username }}</td>
                <td>{{ val.email }}</td>
                <td>{{ val.creat_date }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}

{% block form %}
    <form action="" method="post" enctype="multipart/form-data">
        <p><input type="file" name="file"></p>
        <p><input type="submit" value="Upload"></p>
    </form>
{% endblock %}

{% block script %}{% endblock %}
